#include<cs50.h>
#include<stdio.h>


int main(void)
{
    int n;
    do
    
    {
      
        printf("height:\n");
        n=GetInt();
    }while(n<1||n>23);
   
   for(int i=1 ; i<=n; i++)
   {

         
     
      for(int j=1 /i%i; j<=i ;j++)
      {
          printf("");
          printf("#");
          
      }
        printf("\n");
     
   }
   
}